<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProgressBarActivity">
<com.dinuscxj.progressbar.CircleProgressBar
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_marginTop="20dp"
    app:progress_stroke_width="10dp"
    app:progress_text_color="#fff"
    app:progress_start_color="@color/design_default_color_primary"
    app:progress_end_color="@color/design_default_color_primary_dark"
    android:layout_centerInParent="true"
    app:style="solid_line"
    android:id="@+id/circleProgressBar"/>
</RelativeLayout>